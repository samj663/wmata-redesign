    "cli-progress": "^3.12.0",
    "crypto-browserify": "^3.12.0",
    "decompress": "^4.2.1",
    "dotenv": "^16.1.3",
    "fs": "^0.0.1-security",
    "gtfs-realtime-bindings": "^1.1.1",
    "http": "^0.0.1-security",
    "http-proxy-middleware": "^2.0.6",
    "https": "^1.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-extended": "^4.0.1",
    "linebyline": "^1.3.0",
    "mapbox-gl": "^2.15.0",
    "node-fetch": "^2.7.0",
